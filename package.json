    "koa-body": "^6.0.1",
    "koa-router": "^13.0.1",
    "log4js": "^6.9.1",
    "mysql2": "^3.12.0",
    "pm2": "^5.4.3",
    "qs": "^6.14.0",
    "request": "^2.88.2",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.8",
